{
    "name": "antzy-snippets",
    "type": "module",
    "displayName": "Antzy Snippets",
    "description": "Code snippet of AntzyMo",
    "repository": "https://github.com/AntzyMo/ant-snippets.git",
    "version": "0.0.9",
    "license": "MIT",
    "publisher": "AntzyMo",
    "icon": "images/icon.png",
    "scripts": {
        "release": "pnpm build && bumpp package.json  --commit --push --tag ",
        "build": "node index.js"
    },
    "engines": {
        "vscode": "^1.71.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./build/js.json"
            },
            {
                "language": "typescript",
                "path": "./build/js.json"
            },
            {
                "language": "vue",
                "path": "./build/vue.json"
            }
        ]
    },
    "devDependencies": {
        "@antzy/eslint-config": "^1.4.9",
        "bumpp": "^8.2.1",
        "eslint": "^8.24.0",
        "fsany": "^0.0.3"
    }
}
